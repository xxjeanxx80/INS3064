{
  "version": 2,
  "builds": [
    { "src": "**/*.php", "use": "@vercel/php" },
    { "src": "assets/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
